<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Parallel Testing" parallel="tests" verbose="2">
	<test name="Firefox testing">
		<parameter name="BrowserType" value="firefox"></parameter>
		<classes>
			<class name="com.bioforumis.test.MakeMyTripExecution"/>
		</classes>
	</test>
	<test name="Chrome testing">
		<parameter name="BrowserType" value="chrome"></parameter>
		<classes>
			<class name="com.bioforumis.test.MakeMyTripExecution"/>
		</classes>
	</test>
	
</suite>